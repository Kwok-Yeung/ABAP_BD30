*&---------------------------------------------------------------------*
*&      Module  USER_COMMAND_0100  INPUT
*&---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE user_command_0100 INPUT.
  CASE gf_ok_Code.
    when 'GO'.
      SET SCREEN 200.
  ENDCASE.
ENDMODULE.
*&---------------------------------------------------------------------*
*&      Module  USER_COMMAND_0200  INPUT
*&---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE user_command_0200 INPUT.
  CASE gf_ok_Code.
    when 'BACK'.
      SET SCREEN 100.
    when 'EXIT'.
      SET SCREEN 0.
  ENDCASE.

ENDMODULE.
